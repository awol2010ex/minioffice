<?xml version="1.0" encoding="UTF-8" ?> 

<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="role">
	
    <!-- 查询角色 -->
	<select id="searchRoleList" parameterType="map"  resultType="com.minioffice.user.entity.TBizRoleVO">
	    select * from T_BIZ_ROLE
	</select>	
	<select id="searchRoleCount" parameterType="map"  resultType="int">
	    select count(1) from T_BIZ_ROLE
	</select>	
</mapper>